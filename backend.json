{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^[A-Za-z0-9]{24}$": {
      "type": "object",
      "properties": {
        "age": {
          "type": "string",
          "pattern": "^[0-9]+$"
        },
        "bio": {
          "type": "string"
        },
        "caption": {
          "type": "string"
        },
        "image": {
          "type": "string",
          "format": "uri"
        },
        "name": {
          "type": "string"
        }
      },
      "required": ["age", "name"]
    }
  },
  "additionalProperties": false
}
